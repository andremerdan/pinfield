{
  "name": "pinfield",
  "version": "1.1.5",
  "description": "A react-library to display and handle lock screens",
  "author": "tendo14",
  "license": "MIT",
  "repository": "tendo14/pinfield",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/tendo14/pinfield",
  "bugs": {
    "url": "https://github.com/tendo14/pinfield/issues"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "tsc --noEmit",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.7",
    "@types/testing-library__dom": "^7.5.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "^3.4.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "react-lineto": "^3.2.0",
    "reakit": "^1.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test:lint && lint-staged",
      "post-checkout": "yarn"
    }
  }
}
